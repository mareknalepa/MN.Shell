<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="AccentBaseColor">#FFCA5100</Color>
    <SolidColorBrush x:Key="AccentBaseBrush" Color="#FFCA5100"/>

    <Color x:Key="AccentActiveColor">#FFB43C00</Color>
    <SolidColorBrush x:Key="AccentActiveBrush" Color="#FFB43C00"/>

    <Color x:Key="AccentHighlightedColor">#FFF07828</Color>
    <SolidColorBrush x:Key="AccentHighlightedBrush" Color="#FFF07828"/>

    <Color x:Key="AccentForegroundColor">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="AccentForegroundBrush" Color="#FFFFFFFF"/>

</ResourceDictionary>
