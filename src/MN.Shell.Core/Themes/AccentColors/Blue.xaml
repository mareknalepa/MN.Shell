<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="AccentBaseColor">#FF007ACC</Color>
    <SolidColorBrush x:Key="AccentBaseBrush" Color="#FF007ACC"/>

    <Color x:Key="AccentActiveColor">#FF005AAA</Color>
    <SolidColorBrush x:Key="AccentActiveBrush" Color="#FF005AAA"/>

    <Color x:Key="AccentHighlightedColor">#FF28A0F0</Color>
    <SolidColorBrush x:Key="AccentHighlightedBrush" Color="#FF28A0F0"/>

    <Color x:Key="AccentForegroundColor">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="AccentForegroundBrush" Color="#FFFFFFFF"/>

</ResourceDictionary>
